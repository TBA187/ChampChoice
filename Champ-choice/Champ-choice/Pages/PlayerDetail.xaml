<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Champ_choice.Pages.PlayerDetail">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Thanks for voting on: "/>
            <Label Text="{Binding playerName}"/>
            <Label Text="With player number: "/>
            <Label Text="{Binding playerNr}"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>